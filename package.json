{
  "name": "claude-usage-share",
  "version": "1.0.0",
  "description": "Privacy-first CLI tool to analyze Claude Code usage with real-time session tracking and model distribution analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cushare": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/cli.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepack": "npm run clean && npm run build && npm test -- --run",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "claude",
    "claude-code", 
    "usage",
    "analytics",
    "cli",
    "logs",
    "typescript",
    "privacy",
    "session-tracking",
    "ai-usage",
    "billing-tracker"
  ],
  "author": "Claude Usage Share Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ranmedina/claude-usage-share.git"
  },
  "homepage": "https://github.com/ranmedina/claude-usage-share#readme",
  "bugs": {
    "url": "https://github.com/ranmedina/claude-usage-share/issues"
  },
  "dependencies": {
    "yargs": "^17.7.2",
    "cli-table3": "^0.6.5",
    "zod": "^3.23.8",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "eslint": "^9.9.1",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "rimraf": "^5.0.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}